{
  "name": "electron-test",
  "version": "0.0.1",
  "description": "Dockyard-as-an-App",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=products electron ./",
    "start-dev": "cross-env ELECTRON_ENABLE_LOGGING=true NODE_ENV=development electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "webpack-dev": "webpack --config webpack.config.development.js --progress --colors",
    "webpack-dev-watch": "webpack --config webpack.config.development.js --progress --watch --colors",
    "lint": "eslint app *.js",
    "hot-server": "node server.js",
    "postinstall": "typings install",
    "dev": "npm run webpack-dev; concurrently --kill-others \"npm run webpack-dev-watch\" \"npm run start-dev\"",
    "dev-hot": "npm run webpack-dev; concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stuf/electron-yard.git"
  },
  "author": "Stefan Rimaila <stefan@rimaila.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stuf/electron-yard/issues"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "redux-logger": "^2.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "electron-debug": "^0.5.2",
    "electron-prebuilt": "^0.37.2",
    "file-loader": "^0.8.5",
    "immutable": "^3.7.6",
    "less-loader": "^2.2.3",
    "postcss": "^5.0.19",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "semantic-ui": "^2.1.8"
  }
}
